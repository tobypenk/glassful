function debounce_EDEHH(func, wait, immediate) {
var timeout;
return function() {
var context = this, args = arguments;
var later = function() {timeout = null; if (!immediate) func.apply(context, args); };
var callNow = immediate && !timeout; clearTimeout(timeout); timeout = setTimeout(later, wait); if (callNow) func.apply(context, args); };
};
var hasVerticalCenter_EDEHH = document.getElementsByClassName('position-centered');
function verticalCenter_EDEHH(element) { if(element) { element.style.opacity = 0;  element.style.display = 'block';  }
setTimeout(function () { if (hasVerticalCenter_EDEHH.length > 0) { var windowHeight = Math.max(document.documentElement.clientHeight, window.innerHeight || 0); 
 var formElement_EDEHH = document.getElementsByClassName('formbox-editor_EDEHH')[0]; var formHeight_EDEHH = formElement_EDEHH.clientHeight; 
 if (formHeight_EDEHH < windowHeight) { var newPosition = 0; newPosition = (windowHeight - formHeight_EDEHH) / 2; formElement_EDEHH.style.top = newPosition + 'px'; } else { formElement_EDEHH.style.top = '0px'; } }
if(element) { element.style.opacity = 1;  }
 }, 100);
}
if (hasVerticalCenter_EDEHH.length > 0) { var resizeEvent_EDEHH = debounce_EDEHH(function() { verticalCenter_EDEHH(); }, 250); window.addEventListener('resize', resizeEvent_EDEHH); } 

