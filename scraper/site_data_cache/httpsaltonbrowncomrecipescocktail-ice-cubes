var getUpdates = document.querySelector('.footer-inner h3').innerHTML;
var myArray = getUpdates.split("");
var loopTimer;
var trigger = document.getElementById('newsletter');
if(trigger){
	trigger.style.cursor = 'pointer';
	trigger.style.textDecoration = 'underline';	
	
function frameLooper() {
if(myArray.length > 0) {
document.querySelector('.footer-inner h3').innerHTML += myArray.shift();
} else {
clearTimeout(loopTimer);
	myArray = getUpdates.split("");
return false;
}
loopTimer = setTimeout('frameLooper()',30);
}
	
trigger.addEventListener('click',function(){
	window.scrollTo(0,document.body.scrollHeight);
	document.querySelector('.footer-inner h3').innerHTML = "";
	frameLooper();
});
}

